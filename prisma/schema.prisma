// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model project {
  id           String  @id @map("_id")
  image_src    String
  title        String
  desc         String
  published_at String?
  skillIds     Int[]
}

model skill {
  id         String @id @map("_id")
  name       String
  image_path String
}
